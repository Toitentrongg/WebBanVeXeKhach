                                <h4>Điền Nơi Bạn Muốn Đến :</h4>
                                <form id="form-submit" action="" method="post">
                                    <div class="row">
                                        <div class="col-md-6">
                                            <fieldset>
                                                <label for="from">Nơi Xuất Phát:</label>
                                                <select style="color: black;height:50px;font-size:x-large" required name='start'>
                                                    <option value="">Select a location...</option>
                                                    <option value="SaiGon">Sài Gòn </option>
                                                    <option value="NhaTrang">Nha Trang</option>
                                                    <option value="PhuYen">Phú Yên</option>
                                                    <option value="DaLat">Đà Lạt</option>
                                                    <option value="VungTau">Vũng Tàu</option>
                                                    <option value="CanTho">Cần Thơ</option>
                                                    <option value="BenTre">Bến Tre</option>
                                                </select>
                                            </fieldset>
                                        </div>
                                        <div class="col-md-6">
                                            <fieldset>
                                                <label for="to">Nơi Đến:</label>
                                                <select style="color: black;height:50px;font-size:x-large" required name='end'>
                                                    <option value="">Select a location...</option>
                                                    <option value="SaiGon">Sài Gòn </option>
                                                    <option value="NhaTrang">Nha Trang</option>
                                                    <option value="PhuYen">Phú Yên</option>
                                                    <option value="DaLat">Đà Lạt</option>
                                                    <option value="VungTau">Vũng Tàu</option>
                                                    <option value="CanTho">Cần Thơ</option>
                                                    <option value="BenTre">Bến Tre</option>
                                                </select>
                                            </fieldset>
                                        </div>
                                        <div style="margin:10px" class="col-md-6">
                                            <fieldset>
                                                <label for="departure">Ngày Đi:</label>
                                                <input style="height:50px;font-size:x-large" name="departure" type="text" class="form-control date" id="departure" placeholder="Select date...">
                                            </fieldset>
                                        </div>
                                        <div style="margin:0px;width:665px" class="col-md-6">
                                            <fieldset>
                                                <button style="height:50px;font-size:x-large" type="submit" id="form-submit" class="btn">Order Ticket Now</button>
                                            </fieldset>
                                        </div>

                                    </div>
                                </form>
